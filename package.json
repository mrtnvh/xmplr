{
  "name": "@mrtnvh/xmplr",
  "version": "1.0.0",
  "description": "Example project to test out Open Source Tooling and automation",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "tsc --project tsconfig.prod.json",
    "test": "run-s test:*",
    "test:lint": "eslint --ext .js,.ts --ignore-pattern !.* --ignore-path .gitignore . --fix",
    "test:unit": "jest --selectProjects unit",
    "test:integration": "jest --selectProjects integration",
    "test:e2e": "playwright test -c tests/e2e",
    "fixture": "ws -z -d tests/e2e/__fixture__",
    "semantic-release": "semantic-release",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vanhoofmaarten/xmplr.git"
  },
  "keywords": [
    "test"
  ],
  "author": "@mrtnvh",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vanhoofmaarten/xmplr/issues"
  },
  "homepage": "https://github.com/vanhoofmaarten/xmplr#readme",
  "files": [
    "dist/*"
  ],
  "devDependencies": {
    "@commitlint/cli": "13.1.0",
    "@commitlint/config-conventional": "13.1.0",
    "@playwright/test": "^1.15.1",
    "@semantic-release/changelog": "6.0.0",
    "@semantic-release/commit-analyzer": "9.0.1",
    "@semantic-release/git": "10.0.0",
    "@semantic-release/npm": "8.0.0",
    "@semantic-release/release-notes-generator": "10.0.2",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "27.0.2",
    "@typescript-eslint/eslint-plugin": "4.31.2",
    "@typescript-eslint/parser": "4.31.2",
    "eslint": "7.32.0",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-config-airbnb-typescript": "14.0.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.24.2",
    "eslint-plugin-jest": "24.4.2",
    "eslint-plugin-prettier": "4.0.0",
    "fs-extra": "^10.0.0",
    "jest": "27.2.2",
    "lint-staged": "11.1.2",
    "local-web-server": "^5.1.1",
    "npm-run-all": "4.1.5",
    "prettier": "2.4.1",
    "rollup": "2.57.0",
    "rollup-plugin-dts": "^4.0.0",
    "rollup-plugin-esbuild": "^4.5.0",
    "semantic-release": "18.0.0",
    "ts-jest": "27.0.5",
    "typescript": "^4.5.0-beta",
    "vuepress": "1.8.2"
  }
}
