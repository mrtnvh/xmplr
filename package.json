{
	"name": "@mrtnvh/xmplr",
	"version": "1.0.0",
	"description": "Example project to test out Open Source Tooling and automation",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"type": "module",
	"scripts": {
		"build": "tsc --project tsconfig.prod.json",
		"test": "run-s test:*",
		"test:format": "prettier . --write",
		"test:lint": "eslint --ext .js,.ts --ignore-pattern !.* --ignore-path .gitignore . --fix",
		"test:unit": "jest --selectProjects unit",
		"test:integration": "jest --selectProjects integration",
		"test:e2e": "playwright test -c tests/e2e",
		"fixture": "ws -z -d tests/e2e/__fixture__",
		"semantic-release": "semantic-release",
		"docs:dev": "vuepress dev docs",
		"docs:build": "vuepress build docs"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/vanhoofmaarten/xmplr.git"
	},
	"keywords": [
		"test"
	],
	"author": "@mrtnvh",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/vanhoofmaarten/xmplr/issues"
	},
	"homepage": "https://github.com/vanhoofmaarten/xmplr#readme",
	"files": [
		"dist/*"
	],
	"devDependencies": {
		"@commitlint/cli": "19.0.3",
		"@commitlint/config-conventional": "19.0.3",
		"@playwright/test": "^1.42.1",
		"@semantic-release/changelog": "6.0.3",
		"@semantic-release/commit-analyzer": "11.1.0",
		"@semantic-release/git": "10.0.1",
		"@semantic-release/npm": "11.0.3",
		"@semantic-release/release-notes-generator": "12.1.0",
		"@types/fs-extra": "^11.0.4",
		"@types/jest": "29.5.12",
		"@typescript-eslint/eslint-plugin": "7.1.0",
		"@typescript-eslint/parser": "7.1.0",
		"eslint": "8.57.0",
		"eslint-config-airbnb-base": "15.0.0",
		"eslint-config-airbnb-typescript": "18.0.0",
		"eslint-config-prettier": "9.1.0",
		"eslint-plugin-import": "2.29.1",
		"eslint-plugin-jest": "27.9.0",
		"eslint-plugin-prettier": "5.1.3",
		"fs-extra": "^11.2.0",
		"jest": "29.7.0",
		"lint-staged": "15.2.2",
		"local-web-server": "^5.3.1",
		"npm-run-all": "4.1.5",
		"prettier": "3.2.5",
		"rollup": "4.12.0",
		"rollup-plugin-dts": "^6.1.0",
		"rollup-plugin-esbuild": "^6.1.1",
		"semantic-release": "23.0.2",
		"ts-jest": "29.1.2",
		"typescript": "^5.3.3",
		"vuepress": "1.9.10"
	},
	"volta": {
		"node": "20.11.1",
		"npm": "10.2.4"
	}
}
